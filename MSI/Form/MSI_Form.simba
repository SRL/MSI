procedure ThreadSafeCallEx(Proc: string);
var
  V: TVariantArray;
begin
  SetLength(V, 0);
  ThreadSafeCall(Proc, V);
end;

procedure FreeMSIForm;
begin
  AnimTimer.Enabled := False;
  AnimTimer.Free;
  script_Form.Free;
  MainForm.Free;
end;

